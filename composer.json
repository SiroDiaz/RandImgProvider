{
    "name": "randimg/faker-randimg-provider",
    "type": "library",
    "description": "Faker PHP Provider. Generate random url images using rand-img.com",
    "keywords": [
        "sirodiaz",
        "faker",
        "random",
        "fake images",
        "faker provider",
        "randimg",
        "random images",
        "images",
        "php"
    ],
    "homepage": "https://github.com/SiroDiaz/RandImgProvider",
    "license": "MIT",
    "authors": [
        {
            "name": "Siro Diaz Palazon",
            "email": "siro_diaz@yahoo.com",
            "homepage": "http://www.rand-img.com/",
            "role": "Founder & Developer"
        }
    ],
    "require": {
        "php": "~5.6|~7.0",
        "fzaninotto/faker": "^1.8"
    },
    "require-dev": {
        "phpunit/phpunit" : ">=5.4.3",
        "squizlabs/php_codesniffer": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "Siro\\RandImg\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Siro\\Tests\\RandImg\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests",
        "fix-style": "phpcbf -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
